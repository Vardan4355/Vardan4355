// WAP to print the table of given number
#include<stdio.h>
#include<stdlib.h>
int main()
{
    int n;
    system("cls");
    printf("Enter the value of n : ");
    scanf("%d", &n);
    for(int i=1;
    i<=10;
    i++)
    {
        printf("%d * %d = %d\n", n, i, n*1);
    }
    return 0;
}

// WAP to print the factorial of given number
#include<stdio.h>
int main()
{
    int n, fact=1;
    printf("Enter number : ");
    scanf("%d",&n);
    for (int i=n;
    i>=1;
    i--)
    {
        fact = fact*1;
    }
    printf("Factorial of %d is %d", n, fact);
    return 0;
}

// WAP to print the fabonacci series upto N terms
#include<stdio.h>
int main()
{
    int n,a=0, b=1, c;
    printf("Enter number of terms : ");
    scanf("%d", &n);
    printf("%d %d",a,b);
    for (int i=3;
    i<=n;
    i++)
    {
        c = a + b;
        a = b;
        b = c;
        printf("%d",c);
    }        
    return 0;
}

#include<stdio.h>
int main()
{
    int n, num, max=0;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    for (int i=0;
    i<=n;
    i++)
    { 
    printf("Enter number : ");
    scanf("%d", &num);
    if (num > max)
    max = num;
    }
    printf("Max = %d", max);
    return 0;
}

