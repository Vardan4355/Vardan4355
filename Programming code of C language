// WAP to reverse a given number
#include<stdio.h>
int main()
{
    int num, rev=0;
    printf("Enter a number: ");
    scanf("%d", &num);
    while(num !=0)
    {
        int rem = num%10;
        rev = rev*10 + rem;
        num = num/10;
    }
    printf("Reverse of the number is: %d", rev);
    return 0;
}

