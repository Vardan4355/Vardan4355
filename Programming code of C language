// WAP to check the given number is a prime number or not
#include<stdio.h>
int main()
{
    int n, i=2, cnt=0;
    printf("Enter a number: ");
    scanf("%d", &n);
    while(i<=n/2)
    {
        if (n%i==0)
        cnt++;
        i++;
    }
    if(cnt==0)
    printf("%d is a prime number", n);
    else
    printf("%d is not a prime number", n);
    return 0;
}

// WAP to check the given number is prime or not
#include<stdio.h>
int main()
{
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);
    if (n <= 1)
    {
        printf("%d is not a prime number.", n);
    }
    else if (n <= 3)
    {
        printf("%d is a prime number.", n);
    }
    else if (n % 2 == 0 || n % 3 == 0)
    {
        printf("%d is not a prime number.", n);
    }
    else
    {
        for (int i = 5; i * i <= n; i += 6)
        {
            if (n % i == 0 || n % (i + 2))
            {
                printf("%d is not a prime number.", n);
            }
            else
            {
                printf("%d is a prime number.", n);
            }
        }
    }
    return 0;
}

// WAP to check the given number is prime or not
#include<stdio.h>
int main()
{
    int n, i=2, cnt=0;
    printf("Enter the value of n: ");
    scanf("%d", &n);
    while(i<=n/2)
    {
        if (n%i == 0)
        cnt++;
        i++;
    
        if(cnt==0)
        {
            printf("%d is not a prime number", n);
        }
        else
        {
            printf("%d is a prime number", n);
        }
    }
    return 0;
}

// WAP to perform the sum of all input numbers until user enter zero
#include<stdio.h>
int main()
{
    int num, sum=0;
    do
    {
        printf("Enter a number: ");
        scanf("%d", &num);
        sum += num;
    }
    while(num !=0);
    printf("Sum of all numbers is: %d", sum);
    return 0;
}

// storage class example
#include<stdio.h>   
int main()
{
    auto int x;   
    {
        int y;
    }
    printf("The value of y = %d", y);
    printf("The value of x = %d\n", x);
    return 0;
}

// Storage class example
#include<stdio.h>
void show()
{
    register int x = 1;
    static int y=1;
    printf("x= %d and y= %d\n", x,y);
    x++;
    y++;
}
int main()
{
     show();
     show();
     show();
    return 0;
}

